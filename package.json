{
    "name": "cheater",
    "displayName": "Cheater",
    "description": "cheatsheet in your editor",
    "version": "0.0.1",
    "publisher": "YAEGER",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "icon": "images/cheatsheet_icon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/Uigeo/Cheater.git"
    },
    "activationEvents": [
        "onCommand:extension.sayHello"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Cheater"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "dependencies": {}
}
